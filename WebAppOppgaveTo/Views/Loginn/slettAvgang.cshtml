


@model Model.Avgang


@{


    if (Session["LoggetInn"] == null)
    {
        Response.Redirect("~/Loginn/Index");
    }

}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Slett</title>
</head>
<body style="background-image: linear-gradient(to top, #cfd9df 0%, #e2ebf0 100%); background-attachment: fixed;">
    <div class="navbar navbar-inverse navbar-fixed-top" style="background-color:black; font-size:18px">
        <div class="container">
            <div class="navbar-header">
                <button data-target="#navbar-collapse-02" data-toggle="collapse" class="navbar-toggle" type="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button><img src="~/Images/Logo.PNG" width="80" height="48" />

            </div>
            <div id="navbar-collapse-02" class="collapse navbar-collapse">
                <ul class="nav navbar-nav"></ul>
                <ul class="nav navbar-nav navbar-right">
                    <li> @Html.ActionLink("Tilbake til Avgang", "HentAlleAvgang", "Loginn")</li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </div>
    </div>


    <br /><br />
    <h3>Bekreft sletting av denne avgangen.</h3>
    <div>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(model => model.FraogTiltid)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FraogTiltid)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Lengdne)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Lengdne)
            </dd>
            <dt>
                @Html.DisplayNameFor(model => model.Price)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Price)
            </dd>

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Slett" class="btn btn-danger" />

            </div>
        }
    </div>
</body>
</html>
